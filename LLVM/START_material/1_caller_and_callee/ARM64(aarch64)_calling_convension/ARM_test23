fun:
        sub     sp, sp, #16
        str     d0, [sp, 8]
        ldr     x0, [sp, 8]
        fmov    d0, x0
        add     sp, sp, 16
        ret
.LC0:
        .string "%lf\n"
main:
        stp     x29, x30, [sp, -32]!
        mov     x29, sp
        adrp    x0, .LC1
        ldr     d0, [x0, #:lo12:.LC1]
        bl      fun
        str     d0, [sp, 24]
        ldr     d0, [sp, 24]
        adrp    x0, .LC0
        add     x0, x0, :lo12:.LC0
        bl      prdoublef
        nop
        ldp     x29, x30, [sp], 32
        ret
.LC1:
        .word   2684354560
        .word   1072798105
		
========================================
double fun(double x)
{
    return x;
}

double main()
{
    double r=fun(1.1f);
    prdoublef("%lf\n" ,r);
}