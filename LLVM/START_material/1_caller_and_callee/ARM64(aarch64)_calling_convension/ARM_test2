fun:
        sub     sp, sp, #16
        str     s0, [sp, 12]
        ldr     w0, [sp, 12]
        fmov    s0, w0
        add     sp, sp, 16
        ret
.LC0:
        .string "%f\n"
main:
        stp     x29, x30, [sp, -32]!
        mov     x29, sp
        mov     w0, 52429
        movk    w0, 0x3f8c, lsl 16
        fmov    s0, w0
        bl      fun
        str     s0, [sp, 28]
        ldr     s0, [sp, 28]
        fcvt    d0, s0
        adrp    x0, .LC0
        add     x0, x0, :lo12:.LC0
        bl      prfloatf
        nop
        ldp     x29, x30, [sp], 32
        ret

=======================================
float fun(float x)
{
    return x;
}

float main()
{
    float r=fun(1.1f);
    prfloatf("%f\n" ,r);
}